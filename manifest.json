{
  "name": "Remember, no emoji",
  "version": "2.137.69",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://*.facebook.com/*","http://*.facebook.com/*"],
	  "run_at": "document_idle",
	  "js": ["remover.js"]
    }
  ],
  "browser_action": {
    "default_icon": "xd.png"
  },
  "permissions": [
      "activeTab"
  ],
  "icons": { "16": "xd.png",
           "48": "xd.png",
          "128": "xd.png"
  }
}